{
  "name": "core23/setlistfm-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides services for using the setlist.fm API inside the symfony.",
  "keywords": [
    "symfony",
    "bundle",
    "setlist.fm",
    "api",
    "bridge",
    "webservice client"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "core23/setlistfm-api": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "symfony/config": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/expression-language": "^4.2",
    "symfony/framework-bundle": "^4.2",
    "symfony/http-foundation": "^4.2",
    "symfony/http-kernel": "^4.2"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.3",
    "localheinz/composer-normalize": "^1.3",
    "nyholm/psr7": "^1.0",
    "symfony/http-client": "^4.4 || ^5.0"
  },
  "suggest": {
    "cache/adapter-bundle": "Symfony cache adapter",
    "symfony/http-client": "Symfony HTTP client implementation"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Core23\\SetlistFmBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\SetlistFmBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  }
}
