<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service alias="nucleos_setlistfm.service.artist" id="Nucleos\SetlistFm\Service\ArtistService" public="true"/>
    <service alias="nucleos_setlistfm.service.country" id="Nucleos\SetlistFm\Service\CountryService" public="true"/>
    <service alias="nucleos_setlistfm.service.city" id="Nucleos\SetlistFm\Service\CityService" public="true"/>
    <service alias="nucleos_setlistfm.service.user" id="Nucleos\SetlistFm\Service\UserService" public="true"/>
    <service alias="nucleos_setlistfm.service.setlist" id="Nucleos\SetlistFm\Service\SetlistService" public="true"/>
    <service alias="nucleos_setlistfm.service.venue" id="Nucleos\SetlistFm\Service\VenueService" public="true"/>
    <service id="nucleos_setlistfm.connection" class="Nucleos\SetlistFm\Connection\PsrClientConnection">
      <argument type="service" id="nucleos_setlistfm.http.client"/>
      <argument type="service" id="nucleos_setlistfm.http.message_factory"/>
      <argument>%nucleos_setlistfm.api.key%</argument>
      <argument>%nucleos_setlistfm.api.endpoint%</argument>
    </service>
    <service id="nucleos_setlistfm.service.artist" class="Nucleos\SetlistFm\Service\ArtistService">
      <argument type="service" id="nucleos_setlistfm.connection"/>
    </service>
    <service id="nucleos_setlistfm.service.country" class="Nucleos\SetlistFm\Service\CountryService">
      <argument type="service" id="nucleos_setlistfm.connection"/>
    </service>
    <service id="nucleos_setlistfm.service.city" class="Nucleos\SetlistFm\Service\CityService">
      <argument type="service" id="nucleos_setlistfm.connection"/>
    </service>
    <service id="nucleos_setlistfm.service.user" class="Nucleos\SetlistFm\Service\UserService">
      <argument type="service" id="nucleos_setlistfm.connection"/>
    </service>
    <service id="nucleos_setlistfm.service.setlist" class="Nucleos\SetlistFm\Service\SetlistService">
      <argument type="service" id="nucleos_setlistfm.connection"/>
    </service>
    <service id="nucleos_setlistfm.service.venue" class="Nucleos\SetlistFm\Service\VenueService">
      <argument type="service" id="nucleos_setlistfm.connection"/>
    </service>
  </services>
</container>
